<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>hello ejs practice</h1>
    <%# <%=data['data']%> %>
    <% for(let i = 0; i < 10; i++) {%>
        <div><%=data[i] * 3%></div>
    <% } %>

    <% const dataArray=[{id:1, text:"데이터1입니다"}, {id:2, text:"데이터2입니다"}] %>
    <%dataArray.forEach(e => {%>
        <div>
            <span><%=`id: ${e.id}`%></span>
            <span><%=e.text%></span>
        </div>
    <%})%>
</body>
</html>